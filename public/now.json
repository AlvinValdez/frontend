{
  "name": "please-frontend",
  "env": {
    "REACT_APP_NODE_ENV": "staging"
  },
  "version": 2,
  "routes": [
    {
      "src": "/static/<url>",
      "dest": "/static/$url"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],
  "builds": [{ "src": "/index.html", "use": "@now/static" }]
}
